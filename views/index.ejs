<html>
  <head>
    <link rel="stylesheet" href="/styles.css">
    <title><%= title %></title>
  </head>
  <body>
    <%  messages.forEach((message) => { %>
      <div class="message-div">
        <div>
          <p>
            <%= message.user %>: "<%= message.text %>"
          </p>
          <p>
            -<%= message.added %>
          </p>
        </div>
        <a href="message/<%= message.id %>"><button>Open</button></a>
      </div>
    <% }); %>
    <a href="new">New message</a>
  </body>
</html>
